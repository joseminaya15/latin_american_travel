						background url(../img/fondo/fondo_header1.jpg) no-repeat center center
					&.imagen2
						background url(../img/fondo/fondo_header2.jpg) no-repeat center center
					&.imagen3
						background url(../img/fondo/fondo_header3.jpg) no-repeat center center
.owl-theme 
	.owl-nav
		flexCenter()
		position absolute
		top 0
		bottom 0
		z-index 0
		width 100%
		.owl-prev
			flexCenter()
			position absolute
			left 0
			background-color $color
			&:hover
				background-color $color
		.owl-next
			flexCenter()
			position absolute
			right 0
			background-color $color
			&:hover
				background-color $color
