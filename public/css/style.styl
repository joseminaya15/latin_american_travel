#principal
	overflow hidden
	padding 0
	position relative
	&.js-fondo--principal
		height calc( 100vh - 86px)
	&.js-fondo--login
		height calc( 100vh - 50px)
	.owl-carousel
		relative()
		height calc( 100vh - 86px)
		width 100%
		z-index 2
		.owl-item
			height calc( 100vh - 86px)
			.item
				height calc( 100vh - 86px)
				.fondo-responsive
					position absolute
					top 0
					right 0
					bottom 0
					left 0
					background-size cover !important
					&.fondo1
						background url(../img/fondo/fondo_header1.jpg) no-repeat center center
					&.fondo2
						background url(../img/fondo/fondo_header2.jpg) no-repeat center center
					&.fondo3
						background url(../img/fondo/fondo_header3.jpg) no-repeat center center
					&:AFTER
						position absolute
						content ''
						top 0
						right 0
						left 0
						z-index 2
						background linear-gradient(#FFFFFF, transparent)
						width 100%
						height 200px
.js-slider
	overflow hidden
	padding 0
	position relative
	height 300px
	.owl-carousel
